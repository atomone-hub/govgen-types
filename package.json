{
  "name": "@atomone/govgen-types",
  "version": "0.1.0",
  "description": "JS and TS types relating to Protocol Buffers used by GovGen",
  "contributors": [
    "Simon Warta <webmaster128@users.noreply.github.com>",
    "Will Clark <willclarktech@users.noreply.github.com>",
    "Dan Lynch <pyramation@gmail.com>",
    "Alex M.<alex.megalokonomos@tendermint.com>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomone-hub/govgen-types"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cosmos/",
    "cosmos_proto/",
    "gogoproto/",
    "google/",
    "govgen/",
    "tendermint/",
    "/binary.*",
    "/helpers.*",
    "/utf8.*",
    "/varint.*",
    "/index.*",
    "*.md",
    "!cosmos-sdk-*/**/*.md",
    "!govgen-*/**/*.md"
  ],
  "scripts": {
    "format": "prettier --write --loglevel warn \"./src/**/*.ts\" \"./scripts/**/*.js\"",
    "codegen": "rm -rf ./src && ./scripts/codegen.js && npm run format",
    "prepare-publishing": "./scripts/prepare-publishing.sh",
    "build": "rm -rf ./build && tsc && npm run prepare-publishing"
  },
  "devDependencies": {
    "@cosmjs/amino": "^0.32.2",
    "@cosmjs/proto-signing": "^0.32.2",
    "@cosmjs/stargate": "^0.32.2",
    "@cosmjs/tendermint-rpc": "^0.32.2",
    "@cosmology/telescope": "^1.4.12",
    "@types/node": "^15.6.2",
    "prettier": "^3.0.3",
    "typescript": "~5.2"
  }
}
